# print("Maximu element in the array:", maximum_in_array(arr))
def manimum_in_array(arr):
    min_element = arr[0]

    for num in arr:
        if num < min_element:
            min_element = num
    return min_element


arr = input("Enter the elements of the array seperated by spaces: ").split()

arr = [int(x) for x in arr]
print("Minimum element in the array:", manimum_in_array(arr))
