def find_index(arr, target):

    for i in range(len(arr)):
        if arr[i] == target:
            return i
    return -1

arr = [1, 3, 5, 5, 6, 6]
target = 5

index = find_index(arr, target)

if index != -1:
    print(f"The first occurence of {target} is at index {index}")
else:
    print(f"{target} is not found in the array")